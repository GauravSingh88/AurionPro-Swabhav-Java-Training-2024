package com.aurionpro.day23.ticTacToe.test;

public class TicTacToeTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		char ticTacToe[][] = new char[3][3];
		
		//initialising
		
		for(int i =1 ; i <=3 ; i++ ) {
			for(int j =1 ; j <=3 ; j++ ) {
				ticTacToe[i][j] = '-';
			}
		}
		
		
		//printing
		
		for(int i =1 ; i <=3 ; i++ ) {
			for(int j =1 ; j <=3 ; j++ ) {
				System.out.print(ticTacToe[i][j] + "\t");
			}
			System.out.println();
		}

	}

}
